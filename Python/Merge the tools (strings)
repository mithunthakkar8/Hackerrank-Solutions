def merge_the_tools(string, k):
    # Loop through the string in steps of k
    for i in range(0, len(string), k):
        # Get a substring of length k starting at index i and convert it to a set to remove duplicates
        substring_char = set(string[i:i+k])

        # Print the substring by joining the unique characters together
        print(''.join(substring_char))
        
if __name__ == '__main__':
    string, k = input(), int(input())
    merge_the_tools(string, k)
